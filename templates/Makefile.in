#
# The Bootstrapped rules
#
clean	:; @rm -f $(CLEAN_FILES)

% : %.sh Makefile
	@mv $< $@; chmod +x $@

% : %.pl Makefile
	@cp $< $@; chmod +x $@

%.sh : %.sh.m4 Makefile
	$(M4) -DM4=$(M4) $(M4_FLAGS) $< > $@

%.texi : %.texi.m4 Makefile
	$(M4) -DM4=$(M4) $(M4_FLAGS) $< > $@

%.mk : %.mk.m4 Makefile
	$(M4) -DM4=$(M4) $(M4_FLAGS) $< > $@

%.pm : %.pm.m4 Makefile
	$(M4) -DM4=$(M4) $(M4_FLAGS) $< > $@

%.plx : %.plx.m4 Makefile
	$(M4) -DM4=$(M4) $(M4_FLAGS) $< > $@

%.pl : %.pl.m4 Makefile
	$(M4) -DM4=$(M4) $(M4_FLAGS) $< > $@
	chmod +x $@

%.pl : %.pl.plx Makefile
	perl $(PERL_FLAGS) $(PP) $(PP_FLAGS) $< > $@; chmod +x $@

%.mk : %.mk.plx Makefile
	perl $(PERL_FLAGS) $(PP) $(PP_FLAGS)  $< > $@

%.m4 : %.m4.plx Makefile
	perl $(PERL_FLAGS) $(PP) $(PP_FLAGS)  $< > $@

%.sh : %.sh.plx Makefile
	perl $(PERL_FLAGS) $(PP) $(PP_FLAGS)  $< > $@; chmod +x $@

.localenv : .localenv.plx Makefile
	perl $(PERL_FLAGS) $(PP) $(PP_FLAGS)  $< > $@

